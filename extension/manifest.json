{
  "manifest_version": 2,

  "name": "IRCTC Tatkal",
  "description": "This extension auto fills forms for IRCTC booking",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [ {
        "all_frames": true,
        "js": [ "jquery.min.js","config.js","content.js" ],
        "matches": [ "http://*/*", "https://*/*" ] 
    } ]
}
 